    "Cookie",
    "Controller",
    "Permission",
    "JavaScript",
    "TypeScript"
  ],
  "author": "Martin Drozd√≠k <ja@martin-drozdik.cz> (http://martin-drozdik.cz)",
  "license": "MIT",
  "devDependencies": {
    "@drozdik.m/unit-test": "^1.2.1",
    "@drozdik.m/web-unit-test": "^1.2.5"
  },
  "dependencies": {
    "@drozdik.m/common-mixins": "^1.0.3",
    "@drozdik.m/cookies-controller": "^1.0.0",
    "@drozdik.m/px-in-rem": "^1.0.1"
  }
